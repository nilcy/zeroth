<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
        http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
  <application>
    <message-bundle>FacesMessages</message-bundle>
    <locale-config>
      <default-locale>ja</default-locale>
      <supported-locale>ja</supported-locale>
    </locale-config>
    <resource-bundle>
      <base-name>/bundle</base-name>
      <var>bundle</var>
    </resource-bundle>
  </application>
  <navigation-rule>
    <from-view-id>/home/news/*</from-view-id>
    <navigation-case>
      <from-outcome>home</from-outcome>
      <to-view-id>/home/news/home.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    <navigation-case>
      <from-outcome>view</from-outcome>
      <to-view-id>/home/news/view.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    <navigation-case>
      <from-outcome>edit</from-outcome>
      <to-view-id>/home/news/edit.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    <navigation-case>
      <from-outcome>saved</from-outcome>
      <to-view-id>/home/news/view.xhtml</to-view-id>
      <redirect>
        <view-param>
          <name>id</name>
          <value>#{flash.id}</value>
        </view-param>
      </redirect>
    </navigation-case>
    <navigation-case>
      <from-outcome>deleted</from-outcome>
      <to-view-id>/home/news/home.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    <navigation-case>
      <from-outcome>cancelled</from-outcome>
      <to-view-id>/home/news/home.xhtml</to-view-id>
      <redirect />
    </navigation-case>
  </navigation-rule>
